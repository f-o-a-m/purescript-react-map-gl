{
  "private": true,
  "dependencies": {
    "react": "^16.13",
    "react-map-gl": "^5.2.8"
  },
  "devDependencies": {
    "browserify": "^16.5.2",
    "create-react-class": "^15.6.3",
    "purescript-psa": "^0.7.3",
    "react-dom": "^16.13"
  },
  "scripts": {
    "build": "spago build",
    "//": "",
    "example-map": "yarn example-map:bundle-app && yarn example-map:browserify",
    "example-map:build": "spago --config example-map-spago.dhall build",
    "example-map:bundle-app": "spago --config example-map-spago.dhall bundle-app --to example/map/dist/map.js",
    "example-map:browserify": "browserify example/map/dist/map.js -o example/map/dist/map.js",
    "example-map:serve": "serve example/map/dist",
    "//": "",
    "example-map-halogen": "yarn example-map-halogen:bundle-app && yarn example-map-halogen:browserify",
    "example-map-halogen:build": "spago --config example-map-halogen-spago.dhall build",
    "example-map-halogen:bundle-app": "spago --config example-map-halogen-spago.dhall bundle-app --to example/map-halogen/dist/map-halogen.js",
    "example-map-halogen:browserify": "browserify example/map-halogen/dist/map-halogen.js -o example/map-halogen/dist/map-halogen.js",
    "example-map-halogen:serve": "serve example/map-halogen/dist",
    "//": "",
    "example-heatmap-halogen": "yarn example-heatmap-halogen:bundle-app && yarn example-heatmap-halogen:browserify",
    "example-heatmap-halogen:build": "spago --config example-heatmap-halogen-spago.dhall build",
    "example-heatmap-halogen:bundle-app": "spago --config example-heatmap-halogen-spago.dhall bundle-app --to example/heatmap-halogen/dist/heatmap-halogen.js",
    "example-heatmap-halogen:browserify": "browserify example/heatmap-halogen/dist/heatmap-halogen.js -o example/heatmap-halogen/dist/heatmap-halogen.js",
    "example-heatmap-halogen:serve": "serve example/heatmap-halogen/dist",
    "//": "",
    "example-signal-halogen": "yarn example-signal-halogen:bundle-app && yarn example-signal-halogen:browserify",
    "example-signal-halogen:build": "spago --config example-signal-halogen-spago.dhall build",
    "example-signal-halogen:bundle-app": "spago --config example-signal-halogen-spago.dhall bundle-app --to example/signal-halogen/dist/signal-halogen.js",
    "example-signal-halogen:browserify": "browserify example/signal-halogen/dist/signal-halogen.js -o example/signal-halogen/dist/signal-halogen.js",
    "example-signal-halogen:serve": "serve example/signal-halogen/dist"
  }
}
